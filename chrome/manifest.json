{
"manifest_version": 2,

  "name": "HackerNewsFeed",
  "description": "This extension shows latest posts on hackernews(news.ycombinator.com)",
  "version": "0.0.1",

  "permissions": [
    "http://hackernewslatestapi.herokuapp.com/",
    "tabs"
  ],
  "short_name": "HackerNewsFeed",
  "icons": { "16": "icon16.png",
              "32": "icon32.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  }
}